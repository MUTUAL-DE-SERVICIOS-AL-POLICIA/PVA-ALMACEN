- case type_model
- when 'users'
  - url_list = users_path
  - url_new = new_user_path
  - url_migration = dbf_users_path
- when 'entities'
  - url_list = entities_path
  - url_new = new_entity_path

.container-fluid
  .row
    .col-sm-3.col-md-2.sidebar.menu_left
      %ul.nav.nav-sidebar
        %li.active
          = link_to t("#{type_model}.title.list"), url_list
        %li
          = link_to t("#{type_model}.title.new"), url_new
        - if %w(users).include?(type_model)
          %li
            = link_to 'Migraci√≥n de Usuarios', url_migration
