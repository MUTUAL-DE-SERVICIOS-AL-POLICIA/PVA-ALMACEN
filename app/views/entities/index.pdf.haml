.title_pdf
  %h3= t("#{controller_name}.title.title")

%table.table
  %thead
    %tr
      %th.separator= Entity.human_attribute_name('code')
      %th= Entity.human_attribute_name('name')
      %th= Entity.human_attribute_name('acronym')
  %tbody
    - @array.each do |entity|
      %tr
        %td= entity.code
        %td= entity.name
        %td= entity.acronym
